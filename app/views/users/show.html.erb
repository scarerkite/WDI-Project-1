<%= link_to "Overview", overview_user_path %>
<%= link_to "Challenges", challenges_user_path %>
<%= link_to "Opponents", opponents_user_path %>


<% if params[:overview].present? %>
  <%= render "overview" %>
<% elsif params[:challenges].present? %>
<%= render "challenge" %>
<% elsif params[:opponents].present? %>
  <%= render "opponent" %>

<% else %>
  <%= render "overview" %>
<% end %>


